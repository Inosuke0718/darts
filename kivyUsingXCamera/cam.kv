#: import XCamera kivy.garden.xcamera.XCamera

FloatLayout:
    orientation: "vertical"
    XCamera:
        id: xcamera
        play: True
        on_picture_taken: app.picture_taken()

    Button:
        text: "change camera!"
        size_hint: .2, .1
        pos_hint: {"center_x": .5}
        on_release: app.change_cam(*args)